#!/bin/bash

# LAMP
function lamp {
	# Update repository information
	show_info 'Updating repository information...'
	sudo apt-get update
	# Install
	show_info 'Installing LAMP...'
	sudo apt-get install -y apache2 php5 mysql-server mysql-client php5-mysql phpmyadmin libapache2-mod-php5
    # Done
    show_success 'Done.'
    whiptail --title "Finished" --msgbox "Installation complete." 8 78
	thirdparty
}
